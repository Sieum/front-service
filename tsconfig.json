{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~apis/*": [
        "src/apis/*"
      ],
      "~components/*": [
        "src/components/*"
      ],
      "~nav/*": [
        "src/nav/*"
      ],
      "~screens/*": [
        "src/screens/*"
      ],
      "~icons/*": [
        "src/static/icons/*"
      ],
      "~images/*": [
        "src/static/images/*"
      ]
    }
  }
}